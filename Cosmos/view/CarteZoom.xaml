<UserControl x:Class="Cosmos.view.CarteZoom"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Cosmos.view"
             mc:Ignorable="d" 
             d:DesignHeight="750" d:DesignWidth="500" VerticalAlignment="Center" HorizontalAlignment="Center">
    <Grid Name="grdCarteZoom" Height="750">

        

        <Button Name="btnJouer" Width="200" Height="50" Content="Jouer" Click="btnJouer_Click"
                Grid.Column="1" VerticalAlignment="Top" Margin="0,10,0,0" />
        <Button Name="btnFermer" Width="200" Height="50" Content="Fermer" Click="btnFermer_Click"
                Grid.Column="1" VerticalAlignment="Top" Margin="0,580,0,0" />

    </Grid>
</UserControl>
